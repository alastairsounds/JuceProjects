{
  "C_Cpp.clang_format_fallbackStyle": "none",
  "C_Cpp.clang_format_style": "file",
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.tabSize": 4
  },
  "files.associations": {
    "*.jsonc": "jsonc",
    "*.k": "cpp",
    "*.tcc": "cpp",
    "*.log": "csv",
    "*.mds": "ini",
    "*.mjb": "ini",
    "*.mm": "cpp"
  }
}
